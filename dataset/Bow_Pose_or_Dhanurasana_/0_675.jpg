:root {
  --primary: #46BAF3;
  --secondary: #F00A71;
  --body-bg-color: #FFFFFF;
  --body-color: #3D3D3D;
  --heading-color: #212121;
  --dark: #212121;
  --header-bg: #FFFFFF;
  --anchor: #46BAF3;
  --anchor-hover: #F00A71;
  --menu-link-color: #FFFFFF;
  --menu-link-hover-color: #FFFFFF;
  --mobile-menu-bg-color: #46BAF3;
  --mobile-menu-color: #FFFFFF;
  --mobile-menu-hover-color: #F00A71;
  --footer-bg-image: https://stylesatlife.com/wp-content/uploads/2020/03/stylesatlife-logo.png;
  --footer-bg-color: #FAFAFA;
  --footer-text-color: #FAFAFA;
  --footer-link-color: #FAFAFA;
  --footer-link-hover-color: #46BAF3;
  
  --sans-serif: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
  --body-size: 14px;
  --body-line-height: 1.5;
  --headings-line-height: 1.1;
  --h1-size: 34px;
  --h2-size: 28px;
  --h3-size: 25px;
  --h4-size: 21px;
  --h5-size: 18px;
  --h6-size: 16px;
  --page-title: 50px;
  --section-title: 42px;
  --btn-size : 16px;
  
  --main-padding: 40px 0;
  --section-padding: 40px 0 0;
  
  --mobile-menu-screen-size: 1023px  --mobile-logo-screen-size: 1023px;
  }
.container {max-width: 1200px;}
a {color: var(--anchor);}
a:hover,a:focus {color: var(--anchor-hover);}

body {
background-color: var(--body-bg-color);
color: var(--body-color);
font-family: var(--sans-serif);
font-size: var(--body-size);
line-height: var(--body-line-height);
}

h1,.h1,h2,.h2,h3,.h3,h4,.h4,h5,.h5,h6,.h6 {margin: 0 0 20px;line-height: var(--headings-line-height);font-weight: 600;color:var(--heading-color);}
h1,.h1 {font-size: var(--h1-size);}
h2,.h2 {font-size: var(--h2-size);}
h3,.h3 {font-size: var(--h3-size);}
h4,.h4 {font-size: var(--h4-size);}
h5,.h5 {font-size: var(--h5-size);}
h6,.h6 {font-size: var(--h6-size);}
.color-default {color: 3D3D3D;}
.color-primary {color: var(--primary);}
.color-secondary {color: var(--secondary);}
.color-black {color: #000;}
.color-white {color: #fff;}
.color-dark {color: var(--dark);}

.bg-primary {background-color: var(--primary);}
.bg-secondary {background-color: var(--secondary);}
.bg-dark {background-color: var(--dark);}
.bg-white {background-color: #fff;}
.bg-black {background-color: #000;}

#main {padding: var(--main-padding); }
#main > section {padding: var(--section-padding); }

.btn {
font-size: 16px;
height: auto;
line-height: 1.1;
padding: 10px 20px;
display: inline-block;
text-decoration: none;
letter-spacing: 0.5px;
border: none;
background-color: #000;
color: #fff;
font-weight: 500;
cursor:pointer;
transition: all .3s ease;
}

.btn:hover,.btn:focus {background-color:#222;}

.btn.btn-small {
padding: 8px 15px;
    font-size: 12px;
}
.btn.btn-large {
font-size: 18px;
padding: 15px 30px;
}
.btn.btn-rounded {border-radius: 20px;}
.btn.btn-small-rounded {border-radius: 4px;}

.btn.btn-primary {background: var(--primary); color: #fff;}

.btn.btn-primary:hover,.btn.btn-primary:focus {background-color: var(--secondary); color: #fff;}

.btn.btn-secondary {background: var(--secondary);color: #fff;}

.btn.btn-secondary:hover,.btn.btn-secondary:focus{background: var(--primary); color: #fff;}

.btn.btn-primary-bordered {
    background: none;
    border: 2px solid var(--primary);
    color: var(--primary);
}

.btn.btn-primary-bordered:hover {
    background: var(--primary);
    border-color:  var(--primary);
    color: #fff;
}

.btn.btn-secondary-bordered {
    background: none;
    border: 2px solid var(--secondary);
    color: var(--secondary);
}

.btn.btn-secondary-bordered:hover {
    background: var(--secondary);
    border-color:  var(--secondary);
    color: #fff;
}

.btn.btn-white-bordered {
    background: none;
    border: 2px solid CurrentColor;
    color: #fff;
}

.btn.btn-white-bordered:hover {
    background: #fff;
    border-color:  #fff;
    color: var(--primary);
}

.btn.btn-black-bordered {
    background: none;
    border: 2px solid CurrentColor;
    color: #000;
}

.btn.btn-black-bordered:hover {
    background: #000;
    border-color:  #000;
    color: #fff;
}

.logo-link {display:block;max-width: 526px;width:100%;}

@media screen and (max-width: 1023px) {

.logo-link {max-width: 526px; }

}
.amp-carousel-button {cursor:pointer;border-radius:50%;}
.amp-carousel-button-prev {
    background-image: url(https://stylesatlife.com/wp-content/themes/stylesatlife/images/left-arrow.svg);
}
.amp-carousel-button-next {
    background-image: url(https://stylesatlife.com/wp-content/themes/stylesatlife/images/right-arrow.svg);
}
span.rating {
display:inline-block;
width: 86px;
height: 17px;
background-size: 100% auto;
margin: 1px 5px;
}

.styled-list li:before {color: var(--primary);}

/*Mobile menu */

@media screen and (max-width:1023px){


}




.bg-overlay-primary:before{

    background-color: rgba(70,186,243,0.65);

}

.bg-overlay-secondary:before{

    background-color: rgba(240,10,113,0.65);

}


svg.color-white, svg .color-white {fill: #fff;}

svg.color-black, svg .color-black {fill: #000;}

svg.color-primary, svg .color-primary {fill: var(--primary);}

svg.color-secondary, svg .color-secondary {fill: var(--secondary);}


svg .stroke-white {stroke: #fff;}

svg .stroke-black {stroke: #000;}

svg .stroke-primary {stroke: var(--primary);}

svg .stroke-secondary {stroke: var(--secondary);}

