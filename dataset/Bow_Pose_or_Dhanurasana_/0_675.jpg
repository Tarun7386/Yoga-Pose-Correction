.error-404 form {
    max-width: 500px;
    margin: 50px auto 0;
    position: relative;
    padding-right: 44px;
}

.error-404 form input {
    border: 1px solid #999;
    height: 45px;
    padding: 0 10px;
    font-size: 16px;
    width: 100%;
    border-right: none;
}

.error-404 form button {
    border: none;
    background: var(--primary); 
    font-size: 18px;
    line-height: 45px;
    color: #fff;
    text-align: center;
    position: absolute;
    right: 0;
    top: 0px;
    z-index: 11;
    padding: 0 10px;
}

.error-404 form button:hover {
    background: var(--secondary);
}
.error-404 form button svg {width: 24px;}
/*mobile protrait*/
@media screen and (max-width:479px){
    .error-404 form button {
        position: static;
        font-size: 16px;
    }
    .error-404 form{
        padding-right: 0px;
    }
    .error-404 form input {
        border: 1px solid #999;
        margin-bottom: 20px;
    }
