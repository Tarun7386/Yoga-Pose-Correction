</div><script type="text/javascript">function initializeAccordion(sectionSelector) {
    var accordions = $(sectionSelector + " .accordion-trigger");

    $(accordions).click(function () {
        var clickedItem = $(this).closest(sectionSelector);

        if (clickedItem.hasClass("active")) {
            clickedItem.removeClass("active");
            clickedItem.find(".accordion-trigger").attr("aria-expanded", "false");
        } else {
            $(accordions).not(clickedItem).closest(sectionSelector).removeClass("active");
            $(accordions).not(clickedItem).attr("aria-expanded", "false");
            clickedItem.addClass("active");
            clickedItem.find(".accordion-trigger").attr("aria-expanded", "true");
        }
    });    

    // Assign ID to the spans
    $(accordions).each(function(index) {
        $(this).find('span').attr('id', 'sect' + (index + 1));
    });
}

function updateFooterAccordion() {
    if ($(window).width() > 768) {
        $(".footer-menu-item-title span").addClass("hidden")
    } else {
        $(".footer-menu-item-title span").removeClass("hidden")
    }
}

$(document).ready(function () {
    initializeAccordion(".pdp-details");
    initializeAccordion(".curriculum-item");
    initializeAccordion(".cpt-faqs-item");
    initializeAccordion(".footer-menu-item");

    // Initial call to set the initial state
    updateFooterAccordion();

    // Call the function on window resize
    $(window).resize(function () {
        updateFooterAccordion();
    });
});

