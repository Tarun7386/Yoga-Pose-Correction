        showTopSearches(defaultsToArray(data.searchBarDefaults.TopSearches), "Top Products");
        // showRelatedProducts(
        //   defaultsToArray(data.searchBarDefaults.RelatedProducts),
        //   "Recommended Products"
        // );
        showRelatedProducts(filterDefaults(data.products), "Recommended Products");
    };

    request.send();

    // Function to put default values into an array
    function defaultsToArray(input) {
        var output = Object.keys(input).map((key) => input[key]);
        return output;
    }

    function filterDefaults(input) {
        var relatedToArray = Object.keys(input).map((key) => input[key]);
        var result = relatedToArray.filter((item) => item.default);
        return result;
    }

    function searchJson(term) {
    var products = Object.values(data.products);
    var bundles = Object.values(data.promodata.bundles);

    if (products.length < 1 && bundles.length < 1) {
        return [];
    }



    const matches = [];

    products.forEach((product) => {
        if (product.title && product.title !== "NASM Elite Package" && product.title.toLowerCase().includes(term.toLowerCase())) {
            if (matches.length < 4) matches.push(product);
        }
    });

    

    if (matches.length < 4) {
        bundles.forEach((bundle) => {
            if (bundle.title && bundle.title.toLowerCase().includes(term.toLowerCase())) {
                if (matches.length < 4) matches.push(bundle);
                console.log("matches: ", matches)
            }
            
        });
       
    }
    return matches;
    
}

    // Search as user types
    $("input.center-block.form-control").on("keyup", function (e) {
        const term = e.target.value.trim();
        if (term.length === 0) {
            showTopSearches(defaultsToArray(data.searchBarDefaults.TopSearches), "Top Products");
            showRelatedProducts(filterDefaults(data.products), "Recommended Products");
            return;
        }

        showTopSearches(searchJson(term), "Top Searches");
        showRelatedProducts(searchJson(term), "Related Products");
    });
})();

