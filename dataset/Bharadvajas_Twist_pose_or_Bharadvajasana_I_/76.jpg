                    eventLabel: 'Impression_'+loginStatus // or 'Impression_loggedOut'
                });
            }
            window.sessionStorage['cbcheck'] = 'true';
            sessionCheck = window.sessionStorage['cbcheck'];
        }
        var timeNow = Math.floor(Date.now() / 1000);
        var endTime = timerExpires;
        var timeDif = null;
        var timeRemains = true;
        if(timeNow < endTime) timeDif = endTime - timeNow;
        else timeRemains = false;
        var displayTime = '';
        if(!timeRemains) {
            clearInterval(cbTimer);
            $('.continuityBar').hide();
            $('body').css('padding-top',0);
        } else {
            var hours = Math.floor((timeDif / 60) / 60);//total number of hours left
            /*console.log('timeNow: '+timeNow);
            console.log('endTime: '+endTime);
            console.log('total seconds left: '+timeDif);// (total seconds left / 60) % 1
