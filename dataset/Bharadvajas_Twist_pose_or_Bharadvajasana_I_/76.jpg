jQuery(document).ready(function($){
    var jsonType = 'defaultData', affiliateType = false;
    setTimeout(function(){
        if(typeof window.location.href.split('?')[1] != 'undefined'){
            var qString = window.location.href.split('?')[1];
            if(typeof qString.split('&') != 'undefined'){
                qString = qString.split('&');
                for(var i=0;i < qString.length; i++){
                    var param = qString[i];
                    var paramKey = param.split('=')[0];
                    var paramVal = param.split('=')[1];
                    if(paramKey == 'utm_medium' && paramVal == 'affiliate') affiliateType = true;
                    if(paramKey == 'utm_campaign' && paramVal == 'abtest') jsonType = paramVal;
                }
            }
        }
        if(!affiliateType) loadWunderkindData(jsonType);
    },1000);
});
