document.addEventListener('DOMContentLoaded', function() {
    document.addEventListener('click', function(event) {
        // Check if the clicked element or any of its ancestors have a 'data-href' attribute
        var dataHrefTarget = event.target;
        while (dataHrefTarget && dataHrefTarget !== document && !dataHrefTarget.hasAttribute('data-href')) {
            dataHrefTarget = dataHrefTarget.parentElement;
        }
        // If a 'data-href' element was clicked, navigate to the URL
        if (dataHrefTarget && dataHrefTarget.hasAttribute('data-href')) {
            var dataHrefLocation = dataHrefTarget.getAttribute('data-href');
            var target = dataHrefTarget.getAttribute('data-target');
            // Check if a target is specified and is "_blank" to open in a new tab
            if (target === '_blank') {
                window.open(dataHrefLocation, target);
            } else {
                window.location.href = dataHrefLocation; // Navigate to the URL in the same tab
            }
        }
    });
});</script>
