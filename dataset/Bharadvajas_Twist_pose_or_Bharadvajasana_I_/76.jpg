            console.log('total minutes left / 60 (hours left): '+(timeDif/60)/60);// (total seconds left / 60) % 1
            console.log('hours: '+hours);*/
            var displaySeconds = Math.round(((timeDif / 60) % 1) * 60);
            displaySeconds = (displaySeconds <= 9) ? "0" + displaySeconds : displaySeconds;
            var displayMinutes = Math.floor((((timeDif / 60) / 60) % 1) * 60);
            displayMinutes = (displayMinutes <= 9) ? "0" + displayMinutes : displayMinutes;
            var displayHours = Math.floor(hours % 24);
            displayHours = (displayHours <= 9) ? "0" + displayHours : displayHours;
            //console.log(displaySeconds);
            if(useThirty) {
                $('.cb_minutesremaining').text(displayMinutes);
                $('.cb_secondsremaining').text(displaySeconds);
            } else {
                //console.log('hours: '+hours);
                if(hours >= 24){
                    //console.log('more than 24 hours seconds: '+displaySeconds);
                    //$('.cb_daysremaining').text(Math.floor(hours/24));
                    $('.cb_hoursremaining').text(displayHours);
                    $('.cb_minutesremaining').text(displayMinutes);
                    $('.cb_secondsremaining').text(displaySeconds);
                } else if(hours < 24 && hours >= 1) {
                    //console.log('less than 24 hours seconds: '+displaySeconds);
                    $('.cb_hoursremaining').text(displayHours);
                    $('.cb_minutesremaining').text(displayMinutes);
                    $('.cb_secondsremaining').text(displaySeconds);
                } else if(hours == 0 && parseInt(displayMinutes) >= 1) {
                    //console.log('no hours seconds: '+displaySeconds);
                    $('.cb_minutesremaining').text(displayMinutes);
                    $('.cb_secondsremaining').text(displaySeconds);
                } else if(hours == 0 && parseInt(displayMinutes) == 0) {
                    $('.cb_secondsremaining').text(displaySeconds);
                }
            }
        }
