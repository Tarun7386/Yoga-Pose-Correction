$(document).ready(function () {

    // When a button inside an element with class 'curriculum-item-title' is clicked
    $('.curriculum-item-title button, .cpt-faqs-item-title button').click(function () {

        // Start a timeout function (delayed execution)
        setTimeout(function () {

            // Calculate the distance from the top of the document to the clicked button, adjusted by 100 pixels
            var offsetTop = $(this).offset().top - 100;

            // Animate the scroll to the calculated position over a duration of 250 milliseconds (0.25 seconds)
            $('html, body').animate({
                scrollTop: offsetTop
            }, 250);

            // Bind 'this' to the current context, and set the timeout to 500 milliseconds (0.5 second)
            // This means the function inside setTimeout will have the same 'this' as the outer function
        }.bind(this), 500);

    });
});

