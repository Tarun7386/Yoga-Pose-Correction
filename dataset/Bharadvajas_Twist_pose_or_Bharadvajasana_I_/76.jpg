            // Check if matrix12month exists
            if (matrix12month) {
                return `${matrix12month}/mo`;
            }
            // Check if there is a discounted price for 12 months and hasPromo is true
            else if (promoPricing?.discount?.month['12month'] && hasPromo === "true") {
                return `${promoPricing?.discount?.month['12month']}/mo`;
            }
            // Check if bundlePromoMonthly exists
            else if (bundlePromoMonthly) {
                return bundlePromoMonthly;
            }
            // Check if promomonthly exists
            else if (promomonthly) {
                return `${promomonthly}/mo`;
            } 
            else if (bundlemonthly) {
                return `${bundlemonthly}/mo`;
            }
            // Otherwise, return the regular price per month
            else {
                // This might present an issue in the future if it doesn't have this key. If it doesn't we will probably have to key off of "monthly" rather than promomonthly
                return `${promoPricing.regular.month['12month']}/mo`;
            }
        }

        // Function to get the full price
        function getFullPrice(bundleFullmonthly, promoPricing, hasPromo, matrixfullmonth, monthly, promomonthly, fullmonthly) {
            // Check if matrixfullmonth exists
            if (matrixfullmonth) {
                return matrixfullmonth;
            }
            // Check if there is a discounted price for 12 months, hasPromo is true, and the discounted price is different from the regular price
            else if (promoPricing?.discount?.month['12month'] && hasPromo === "true" && (promoPricing?.discount?.month['12month'] != promoPricing.regular.month['12month'])) {
                return `${promoPricing.regular.month['12month']}/mo`;
            }
            // Check if bundleFullmonthly exists
            else if (bundleFullmonthly) {
                return bundleFullmonthly;
            }
            // Check if monthly exists
            else if (monthly && monthly != promomonthly) {
                return `${monthly}/mo`;
            }
            else if (fullmonthly) {
                return fullmonthly;
            }
            // Otherwise, return an empty string
            else {
                return "";
            }
        }

        console.log('matches 2? ', matches);
        const items = matches
            .map(({ pdpPageUrl, productImage, title, hasPromo, promoprice, regularprice, gaTag, monthly, promomonthly, promoPricing, bundleFullmonthly, bundlePromoMonthly, matrixfullmonth, matrix12month, fullmonthly, bundlemonthly }) => {
