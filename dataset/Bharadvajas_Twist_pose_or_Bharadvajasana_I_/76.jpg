(function () {
    let data;
    const request = new XMLHttpRequest();

    request.open(
        "GET",
        "/docs/nasmlibraries/json/nasm-promo-pricing.json"
        // "http://127.0.0.1:5500/nasm-promo-pricing.json"
    );

